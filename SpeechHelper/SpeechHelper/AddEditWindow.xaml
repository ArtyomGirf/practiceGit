        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpeechHelper"
        mc:Ignorable="d"
        Title="Добавить/Изменить" Height="350" Width="800" MaxHeight="450" MaxWidth="1000">
    <Grid Background="#FF15AFFF">
        <Border Background="White" Margin="20" CornerRadius="20" BorderThickness="2" BorderBrush="#FF00298C">
            <Frame x:Name="addEditPage" NavigationUIVisibility="Hidden"/>
        </Border>
    </Grid>
</Window>
